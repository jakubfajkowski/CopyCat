<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="table" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TableController">
    <columns>
        <TableColumn fx:id="name" editable="false" prefWidth="150.0" text="Name" />
        <TableColumn fx:id="size" prefWidth="75.0" text="Size" />
        <TableColumn fx:id="extension" prefWidth="75.0" text="Extension" />
        <TableColumn fx:id="lastModified" editable="false" prefWidth="100.0" text="Last Modified" />
        <TableColumn fx:id="path" minWidth="-1.0" prefWidth="300.0" text="Path" />
    </columns>
</TableView>
